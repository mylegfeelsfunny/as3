package com.newproject.controller{	import com.newproject.AssetManager;	import com.newproject.view.LoaderMediator;	import org.puremvc.as3.interfaces.ICommand;	import org.puremvc.as3.interfaces.INotification;	import org.puremvc.as3.patterns.command.SimpleCommand;		public final class ViewComponentsCommand extends SimpleCommand implements ICommand	{		override public function execute(notification:INotification):void		{			super.execute(notification);			facade.registerMediator(new LoaderMediator(notification.getBody() as AssetManager));			trace('ViewComponentsCommand');		}	}}