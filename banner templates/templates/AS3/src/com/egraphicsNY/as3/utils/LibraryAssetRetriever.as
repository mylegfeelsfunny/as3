package com.egraphicsNY.as3.utils{	import flash.display.DisplayObject;		public class LibraryAssetRetriever{		public function LibraryAssetRetriever(){}				public static function getDisplayObject($libraryClip:DisplayObject,$assetID:String):DisplayObject{			var assetClass:Class	= Class($libraryClip.loaderInfo.applicationDomain.getDefinition($assetID));			return new assetClass();		}		public static function getClass($libraryClip:DisplayObject,$assetID:String):Class{			return Class($libraryClip.loaderInfo.applicationDomain.getDefinition($assetID));		}	}}