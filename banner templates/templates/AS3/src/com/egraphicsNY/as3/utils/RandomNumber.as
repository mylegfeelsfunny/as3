package com.egraphicsNY.as3.utils{	public class RandomNumber{		public function RandomNumber(){}				public static function getRandom(min_num:Number,max_num:Number):Number{			return min_num + Math.floor(Math.random() * (max_num + 1 - min_num));		}			public static function getRandomFloat(min_num:Number,max_num:Number):Number{			return min_num + (Math.random() * (max_num - min_num));		}			}}